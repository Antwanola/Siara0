<h1>Users List</h1>


 
 <!-- Example DataTables Card-->
      <div class="card mb-3">
        <div class="card-header">
          <i class="fa fa-table"></i>Users List</div>
        <div class="card-body">
          <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
              <thead>
                <tr>
                  <th>Name</th>
                  <th>Phone</th>
                  <th>Email</th>
                  <th>Role</th>
                  <th>Edit</th>
                  <th>Delete</th>
                  
                </tr>
              </thead>
              <tfoot>
                <tr>
                 <th>Name</th>
                  <th>Phone</th>
                  <th>Email</th>
                  <th>Role</th>
                  <th>Edit</th>
                  <th>Delete</th>
                </tr>
              </tfoot>
              <tbody>
                {{#each payload}}
                <tr>
                  <td>{{name}}</td>
                  <td>{{phone}}</td>
                  <td>{{email}}<br>{{date}}</td>
                  <td>{{role}}</td>
                   <td><a href="/admin/users/edit/{{_id}}" class="btn btn-info">Edit</a></td>
                 <td> 
                    <form action="/admin/environmental/delete/{{_id}}?_method=DELETE" method="post">

                    <button type="submit" class="btn btn-danger">DELETE</button>
                </form></td>
             
                  
                </tr>
                {{/each}}
               
               
               
              </tbody>
            </table>
          </div>
        </div>
        <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div>
      </div>
    </div>